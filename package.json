{
  "name": "farm-to-table-theme",
  "displayName": "Farm to Table",
  "description": "A warm, organic color theme inspired by fresh-picked vegetables at the farmers market. Featuring spinach greens, carrot oranges, apple reds, and earthy soil tones.",
  "version": "1.0.0",
  "publisher": "chronick",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "color-theme",
    "dark",
    "light",
    "organic",
    "warm",
    "nature",
    "vegetables",
    "farm",
    "earthy",
    "cooklang",
    "markdown"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Farm to Table Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/farm-to-table-dark.json"
      },
      {
        "label": "Farm to Table Light",
        "uiTheme": "vs",
        "path": "./themes/farm-to-table-light.json"
      }
    ],
    "languages": [
      {
        "id": "cooklang",
        "aliases": ["Cooklang", "cooklang"],
        "extensions": [".cook", ".menu"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "cooklang-conf",
        "aliases": ["Cooklang Config", "aisle.conf"],
        "filenames": ["aisle.conf"],
        "extensions": [".conf"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cooklang",
        "scopeName": "source.cooklang",
        "path": "./syntaxes/cooklang.tmLanguage.json"
      },
      {
        "language": "cooklang-conf",
        "scopeName": "source.cooklang-conf",
        "path": "./syntaxes/cooklang-conf.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chronick/farm-to-table-theme"
  },
  "homepage": "https://chronick.github.io/farm-to-table-theme/",
  "bugs": {
    "url": "https://github.com/chronick/farm-to-table-theme/issues"
  },
  "galleryBanner": {
    "color": "#1a1611",
    "theme": "dark"
  }
}
