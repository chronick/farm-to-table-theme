{
  "name": "endless-bounty",
  "displayName": "Endless Bounty",
  "description": "A warm, organic VS Code color theme inspired by fresh-picked vegetables at the farmers market. Featuring spinach greens, carrot oranges, apple reds, and earthy soil tones.",
  "version": "1.0.1",
  "publisher": "chronick",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "color-theme",
    "dark",
    "light",
    "organic",
    "warm",
    "nature",
    "vegetables",
    "farm",
    "earthy",
    "cooklang",
    "markdown"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Endless Bounty Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/endless-bounty-dark.json"
      },
      {
        "label": "Endless Bounty Light",
        "uiTheme": "vs",
        "path": "./themes/endless-bounty-light.json"
      }
    ],
    "languages": [
      {
        "id": "cooklang",
        "aliases": [
          "Cooklang",
          "cooklang"
        ],
        "extensions": [
          ".cook",
          ".menu"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "cooklang-conf",
        "aliases": [
          "Cooklang Config",
          "aisle.conf"
        ],
        "filenames": [
          "aisle.conf"
        ],
        "extensions": [
          ".conf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cooklang",
        "scopeName": "source.cooklang",
        "path": "./syntaxes/cooklang.tmLanguage.json"
      },
      {
        "language": "cooklang-conf",
        "scopeName": "source.cooklang-conf",
        "path": "./syntaxes/cooklang-conf.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chronick/endless-bounty.git"
  },
  "homepage": "https://chronick.github.io/endless-bounty/",
  "bugs": {
    "url": "https://github.com/chronick/endless-bounty/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1a1611",
    "theme": "dark"
  },
  "scripts": {
    "dev": "npx @11ty/eleventy --serve --input=site --output=docs",
    "build": "npx @11ty/eleventy --input=site --output=docs"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2"
  }
}
