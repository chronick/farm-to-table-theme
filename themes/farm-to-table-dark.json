    "sideBarSectionHeader.background": "#1a1510",
    "sideBarSectionHeader.foreground": "#8cb369",
    "sideBarSectionHeader.border": "#2a231c",

    "list.activeSelectionBackground": "#33291f",
    "list.activeSelectionForeground": "#e0c4a8",
    "list.hoverBackground": "#241d17",
    "list.focusBackground": "#2d2419",
    "list.highlightForeground": "#8cb369",
    "list.inactiveSelectionBackground": "#28211a",

    "titleBar.activeBackground": "#0f0c09",
    "titleBar.activeForeground": "#c9b896",
    "titleBar.inactiveBackground": "#0d0a08",
    "titleBar.inactiveForeground": "#6a6258",
    "titleBar.border": "#1a1510",

    "statusBar.background": "#261810",
    "statusBar.foreground": "#d4c4a8",
    "statusBar.border": "#3a2a1e",
    "statusBar.debuggingBackground": "#8b3a3a",
    "statusBar.debuggingForeground": "#f0dcc8",
    "statusBar.noFolderBackground": "#1a1510",
    "statusBarItem.hoverBackground": "#3a2818",
    "statusBarItem.prominentBackground": "#8cb369",
    "statusBarItem.prominentForeground": "#1a1611",
    "statusBarItem.remoteBackground": "#6a9f8e",
    "statusBarItem.remoteForeground": "#0d0a08",

    "tab.activeBackground": "#171311",
    "tab.activeForeground": "#e0c4a8",
    "tab.inactiveBackground": "#100d0a",
    "tab.inactiveForeground": "#8a8070",
    "tab.border": "#1a1510",
    "tab.activeBorder": "#171311",
    "tab.activeBorderTop": "#8cb369",
    "tab.unfocusedActiveBorderTop": "#5a7a45",
    "editorGroupHeader.tabsBackground": "#0f0c09",
    "editorGroupHeader.tabsBorder": "#251f1a",

    "input.background": "#1a1510",
    "input.foreground": "#e0c4a8",
    "input.border": "#3a3025",
    "input.placeholderForeground": "#6a6258",
    "inputOption.activeBorder": "#8cb369",
    "inputOption.activeBackground": "#8cb36930",
    "focusBorder": "#8cb36980",

    "button.background": "#8cb369",
    "button.foreground": "#0d0a08",
    "button.hoverBackground": "#9dc47a",
    "button.secondaryBackground": "#3a3025",
    "button.secondaryForeground": "#e0c4a8",
    "button.secondaryHoverBackground": "#4a4035",

    "dropdown.background": "#1a1510",
    "dropdown.foreground": "#e0c4a8",
    "dropdown.border": "#3a3025",
    "dropdown.listBackground": "#13100d",

    "scrollbarSlider.background": "#4a403560",
    "scrollbarSlider.hoverBackground": "#5a504580",
    "scrollbarSlider.activeBackground": "#6a6055",

    "badge.background": "#8cb369",
    "badge.foreground": "#0d0a08",

    "terminal.background": "#12100d",
    "terminal.foreground": "#e0c4a8",
    "terminal.border": "#2a231c",
    "terminal.ansiBlack": "#1a1611",
    "terminal.ansiRed": "#c75d5d",
    "terminal.ansiGreen": "#8cb369",
    "terminal.ansiYellow": "#d4a84b",
    "terminal.ansiBlue": "#7a9cbf",
    "terminal.ansiMagenta": "#9b6b9e",
    "terminal.ansiCyan": "#6a9f8e",
    "terminal.ansiWhite": "#e0c4a8",
    "terminal.ansiBrightBlack": "#6a6258",
    "terminal.ansiBrightRed": "#d77070",
    "terminal.ansiBrightGreen": "#9dc47a",
    "terminal.ansiBrightYellow": "#e0b95c",
    "terminal.ansiBrightBlue": "#8aaed0",
    "terminal.ansiBrightMagenta": "#b080b3",
    "terminal.ansiBrightCyan": "#7ab3a0",
    "terminal.ansiBrightWhite": "#f0dcc8",

    "gitDecoration.addedResourceForeground": "#8cb369",
    "gitDecoration.modifiedResourceForeground": "#d4a84b",
    "gitDecoration.deletedResourceForeground": "#c75d5d",
    "gitDecoration.untrackedResourceForeground": "#6a9f8e",
    "gitDecoration.ignoredResourceForeground": "#5a5248",
    "gitDecoration.conflictingResourceForeground": "#e07b39",

    "editorError.foreground": "#c75d5d",
    "editorWarning.foreground": "#d4a84b",
    "editorInfo.foreground": "#7a9cbf",
    "editorHint.foreground": "#6a9f8e",

    "peekView.border": "#8cb369",
    "peekViewEditor.background": "#13100d",
    "peekViewResult.background": "#13100d",
    "peekViewTitle.background": "#1a1510",
    "peekViewTitleLabel.foreground": "#e0c4a8",

    "panel.background": "#100d0a",
    "panel.border": "#2a231c",
    "panelTitle.activeBorder": "#8cb369",
    "panelTitle.activeForeground": "#e0c4a8",
    "panelTitle.inactiveForeground": "#7d8471",
    "panelSectionHeader.background": "#1a1510",

    "breadcrumb.foreground": "#8a8070",
    "breadcrumb.focusForeground": "#e0c4a8",
    "breadcrumb.activeSelectionForeground": "#8cb369",
    "breadcrumbPicker.background": "#13100d",

    "editorWidget.background": "#13100d",
    "editorWidget.border": "#2a231c",
    "editorSuggestWidget.background": "#13100d",
    "editorSuggestWidget.border": "#2a231c",
    "editorSuggestWidget.selectedBackground": "#33291f",
    "editorSuggestWidget.highlightForeground": "#8cb369",
    "editorHoverWidget.background": "#13100d",
    "editorHoverWidget.border": "#2a231c",

    "minimap.background": "#12100d",
    "minimap.selectionHighlight": "#8cb36960",
    "minimapSlider.background": "#4a403530",
    "minimapSlider.hoverBackground": "#4a403550",
    "minimapSlider.activeBackground": "#4a403570",

    "diffEditor.insertedTextBackground": "#8cb36925",
    "diffEditor.removedTextBackground": "#c75d5d25",
    "diffEditorGutter.insertedLineBackground": "#8cb36940",
    "diffEditorGutter.removedLineBackground": "#c75d5d40",

    "editorGhostText.foreground": "#7d847180",

    "inlineChat.background": "#1a1510",
    "inlineChat.border": "#3a3025",

    "editorInlayHint.background": "#9b6b9e20",
    "editorInlayHint.foreground": "#9b6b9e",

    "merge.currentHeaderBackground": "#8cb36940",
    "merge.currentContentBackground": "#8cb36920",
    "merge.incomingHeaderBackground": "#7a9cbf40",
    "merge.incomingContentBackground": "#7a9cbf20",

    "quickInput.background": "#13100d",
    "quickInputList.focusBackground": "#33291f",
    "quickInputTitle.background": "#1a1510",

    "notifications.background": "#1a1510",
    "notifications.foreground": "#e0c4a8",
    "notifications.border": "#2a231c",
    "notificationCenterHeader.background": "#13100d",

    "menu.background": "#13100d",
    "menu.foreground": "#c9b896",
    "menu.selectionBackground": "#33291f",
    "menu.selectionForeground": "#e0c4a8",
    "menu.separatorBackground": "#2a231c",
    "menubar.selectionBackground": "#33291f",

    "commandCenter.background": "#1a1510",
    "commandCenter.foreground": "#c9b896",
    "commandCenter.border": "#2a231c",
    "commandCenter.activeBackground": "#2a231c",
    "commandCenter.activeForeground": "#e0c4a8"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7d8471", "fontStyle": "italic" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#e07b39" }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#8cb369" }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": { "foreground": "#c75d5d" }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": { "foreground": "#d4a84b" }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#9b6b9e" }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#e0a458" }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": { "foreground": "#bf4b4b" }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "meta.object-literal.key", "entity.name.tag.yaml"],
      "settings": { "foreground": "#e0c4a8" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#a89880" }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#c9b896" }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#e0c4a8", "fontStyle": "italic" }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": { "foreground": "#c75d5d" }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#d4a84b" }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": { "foreground": "#7a9cbf" }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": { "foreground": "#8cb369" }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#6a9f8e" }
    },
    {
      "name": "Escape",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#6a9f8e" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#c75d5d", "fontStyle": "underline" }
    },
    {
      "name": "Markdown Heading",
      "scope": ["heading.1.markdown", "markup.heading.setext.1", "entity.name.section.markdown"],
      "settings": { "foreground": "#e07b39", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 2",
      "scope": ["heading.2.markdown", "markup.heading.setext.2"],
      "settings": { "foreground": "#c75d5d", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 3",
      "scope": ["heading.3.markdown"],
      "settings": { "foreground": "#d4a84b", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 4",
      "scope": ["heading.4.markdown"],
      "settings": { "foreground": "#8cb369", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 5",
      "scope": ["heading.5.markdown"],
      "settings": { "foreground": "#9b6b9e", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 6",
      "scope": ["heading.6.markdown"],
      "settings": { "foreground": "#7a9cbf", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#d4a84b", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#9b6b9e", "fontStyle": "italic" }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": { "foreground": "#7a9cbf" }
    },
    {
      "name": "Markdown Link Text",
      "scope": ["string.other.link.title.markdown", "string.other.link.description.markdown"],
      "settings": { "foreground": "#8cb369" }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#6a9f8e" }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": { "foreground": "#7d8471", "fontStyle": "italic" }
    },
    {
      "name": "Markdown List",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": { "foreground": "#e07b39" }
    },
    {
      "name": "Cooklang Ingredient",
      "scope": ["entity.name.ingredient.cooklang", "punctuation.definition.ingredient.cooklang"],
      "settings": { "foreground": "#8cb369", "fontStyle": "bold" }
    },
    {
      "name": "Cooklang Cookware",
      "scope": ["entity.name.cookware.cooklang", "punctuation.definition.cookware.cooklang"],
      "settings": { "foreground": "#c75d5d", "fontStyle": "bold" }
    },
    {
      "name": "Cooklang Timer",
      "scope": ["entity.name.timer.cooklang", "punctuation.definition.timer.cooklang"],
      "settings": { "foreground": "#9b6b9e" }
    },
    {
      "name": "Cooklang Amount",
      "scope": ["constant.numeric.cooklang"],
      "settings": { "foreground": "#e0a458" }
    },
    {
      "name": "Cooklang Unit",
      "scope": ["keyword.other.unit.cooklang"],
      "settings": { "foreground": "#d4a84b" }
    },
    {
      "name": "Cooklang Metadata Key",
      "scope": ["entity.name.tag.metadata.cooklang"],
      "settings": { "foreground": "#7a9cbf" }
    },
    {
      "name": "Cooklang Metadata Value",
      "scope": ["string.unquoted.metadata.cooklang"],
      "settings": { "foreground": "#6a9f8e" }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#7a9cbf" }
    },
    {
      "name": "YAML Key",
      "scope": ["entity.name.tag.yaml"],
      "settings": { "foreground": "#7a9cbf" }
    },
    {
      "name": "This/Self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": { "foreground": "#bf4b4b", "fontStyle": "italic" }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.from", "keyword.control.export"],
      "settings": { "foreground": "#e07b39" }
    },
    {
      "name": "Module",
      "scope": ["entity.name.module", "entity.name.namespace"],
      "settings": { "foreground": "#d4a84b" }
    }
  ]
}
