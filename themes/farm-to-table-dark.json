    "editor.findMatchBackground": "#e07b3950",
    "editor.findMatchHighlightBackground": "#8cb36935",
    "editorBracketMatch.background": "#3d352d",
    "editorBracketMatch.border": "#8cb369",
    "editorIndentGuide.background1": "#2a2420",
