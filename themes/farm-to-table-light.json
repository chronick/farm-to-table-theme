{
  "$schema": "vscode://schemas/color-theme",
  "name": "Farm to Table Light",
  "type": "light",
  "colors": {
    "editor.background": "#fefcf8",
    "editor.foreground": "#3a3225",
    "editorLineNumber.foreground": "#b0a088",
    "editorLineNumber.activeForeground": "#3d7a28",
    "editorCursor.foreground": "#d45a10",
    "editor.selectionBackground": "#c8dba8",
    "editor.selectionHighlightBackground": "#c8dba870",
    "editor.lineHighlightBackground": "#f8f5ef",
    "editor.wordHighlightBackground": "#dde8c8",
    "editor.findMatchBackground": "#f0a06050",
    "editor.findMatchHighlightBackground": "#b8d89050",
    "editorBracketMatch.background": "#c8dba8",
    "editorBracketMatch.border": "#3d7a28",
    "editorIndentGuide.background1": "#e8e2d8",
    "editorIndentGuide.activeBackground1": "#c0a880",
    "editorWhitespace.foreground": "#d8d0c0",

    "activityBar.background": "#3a3028",
    "activityBar.foreground": "#f5f0e8",
    "activityBar.inactiveForeground": "#a09080",
    "activityBar.border": "#4a4038",
    "activityBarBadge.background": "#3d7a28",
    "activityBarBadge.foreground": "#ffffff",

    "sideBar.background": "#f5f2eb",
    "sideBar.foreground": "#3a3225",
    "sideBar.border": "#ddd5c5",
    "sideBarTitle.foreground": "#3a3225",
    "sideBarSectionHeader.background": "#ebe5da",
    "sideBarSectionHeader.foreground": "#3d7a28",
    "sideBarSectionHeader.border": "#d5cbb8",

    "list.activeSelectionBackground": "#c8dba8",
    "list.activeSelectionForeground": "#2a2520",
    "list.hoverBackground": "#ebe5da",
    "list.focusBackground": "#d5e8c0",
    "list.highlightForeground": "#2d6a1a",
    "list.inactiveSelectionBackground": "#ddd8c8",

    "titleBar.activeBackground": "#4a3d32",
    "titleBar.activeForeground": "#f5f0e8",
    "titleBar.inactiveBackground": "#3a3028",
    "titleBar.inactiveForeground": "#b0a090",
    "titleBar.border": "#5a4d42",

    "statusBar.background": "#a85a3a",
    "statusBar.foreground": "#fff8f0",
    "statusBar.border": "#b86a4a",
    "statusBar.debuggingBackground": "#c03030",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.noFolderBackground": "#8a5030",
    "statusBarItem.hoverBackground": "#b86a4a",
    "statusBarItem.prominentBackground": "#3d7a28",
    "statusBarItem.prominentForeground": "#ffffff",
    "statusBarItem.remoteBackground": "#2a8a6a",
    "statusBarItem.remoteForeground": "#ffffff",

    "tab.activeBackground": "#fefcf8",
    "tab.activeForeground": "#3a3225",
    "tab.inactiveBackground": "#f0ebe0",
    "tab.inactiveForeground": "#6a5d50",
    "tab.border": "#ddd5c5",
    "tab.activeBorder": "#fefcf8",
    "tab.activeBorderTop": "#3d7a28",
    "tab.unfocusedActiveBorderTop": "#6a9a50",
    "editorGroupHeader.tabsBackground": "#e8e0d5",
    "editorGroupHeader.tabsBorder": "#d5cbb8",

    "input.background": "#fefcf8",
    "input.foreground": "#3a3225",
    "input.border": "#c5b8a0",
    "input.placeholderForeground": "#908070",
    "inputOption.activeBorder": "#3d7a28",
    "inputOption.activeBackground": "#3d7a2830",
    "focusBorder": "#3d7a2890",

    "button.background": "#3d7a28",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#4d8a38",
    "button.secondaryBackground": "#d5cbb8",
    "button.secondaryForeground": "#3a3225",
    "button.secondaryHoverBackground": "#c5baa5",

    "dropdown.background": "#fefcf8",
    "dropdown.foreground": "#3a3225",
    "dropdown.border": "#c5b8a0",
    "dropdown.listBackground": "#faf8f2",

    "scrollbarSlider.background": "#a0906050",
    "scrollbarSlider.hoverBackground": "#908060",
    "scrollbarSlider.activeBackground": "#807050",

    "badge.background": "#3d7a28",
    "badge.foreground": "#ffffff",

    "terminal.background": "#faf8f2",
    "terminal.foreground": "#3a3225",
    "terminal.border": "#ddd5c5",
    "terminal.ansiBlack": "#3a3225",
    "terminal.ansiRed": "#c03030",
    "terminal.ansiGreen": "#3d7a28",
    "terminal.ansiYellow": "#a06010",
    "terminal.ansiBlue": "#3060a0",
    "terminal.ansiMagenta": "#8040a0",
    "terminal.ansiCyan": "#208070",
    "terminal.ansiWhite": "#f0ebe0",
    "terminal.ansiBrightBlack": "#706050",
    "terminal.ansiBrightRed": "#d04040",
    "terminal.ansiBrightGreen": "#4d8a38",
    "terminal.ansiBrightYellow": "#b07020",
    "terminal.ansiBrightBlue": "#4070b0",
    "terminal.ansiBrightMagenta": "#9050b0",
    "terminal.ansiBrightCyan": "#309080",
    "terminal.ansiBrightWhite": "#fefcf8",

    "gitDecoration.addedResourceForeground": "#3d7a28",
    "gitDecoration.modifiedResourceForeground": "#a06010",
    "gitDecoration.deletedResourceForeground": "#c03030",
    "gitDecoration.untrackedResourceForeground": "#208070",
    "gitDecoration.ignoredResourceForeground": "#a09888",
    "gitDecoration.conflictingResourceForeground": "#d45a10",

    "editorError.foreground": "#c03030",
    "editorWarning.foreground": "#a06010",
    "editorInfo.foreground": "#3060a0",
    "editorHint.foreground": "#208070",

    "peekView.border": "#3d7a28",
    "peekViewEditor.background": "#faf8f2",
    "peekViewResult.background": "#f0ebe0",
    "peekViewTitle.background": "#e8e0d5",
    "peekViewTitleLabel.foreground": "#3a3225",

    "panel.background": "#f0ebe0",
    "panel.border": "#d5cbb8",
    "panelTitle.activeBorder": "#3d7a28",
    "panelTitle.activeForeground": "#3a3225",
    "panelTitle.inactiveForeground": "#807060",
    "panelSectionHeader.background": "#e8e0d5",

    "breadcrumb.foreground": "#706050",
    "breadcrumb.focusForeground": "#3a3225",
    "breadcrumb.activeSelectionForeground": "#3d7a28",
    "breadcrumbPicker.background": "#faf8f2",

    "editorWidget.background": "#faf8f2",
    "editorWidget.border": "#d5cbb8",
    "editorSuggestWidget.background": "#faf8f2",
    "editorSuggestWidget.border": "#d5cbb8",
    "editorSuggestWidget.selectedBackground": "#c8dba8",
    "editorSuggestWidget.highlightForeground": "#2d6a1a",
    "editorHoverWidget.background": "#faf8f2",
    "editorHoverWidget.border": "#d5cbb8",

    "minimap.background": "#fcfaf5",
    "minimap.selectionHighlight": "#3d7a2860",
    "minimapSlider.background": "#a0906030",
    "minimapSlider.hoverBackground": "#a0906050",
    "minimapSlider.activeBackground": "#a0906070",

    "diffEditor.insertedTextBackground": "#3d7a2830",
    "diffEditor.removedTextBackground": "#c0303030",
    "diffEditorGutter.insertedLineBackground": "#3d7a2850",
    "diffEditorGutter.removedLineBackground": "#c0303050",

    "editorGhostText.foreground": "#80706080",

    "inlineChat.background": "#faf8f2",
    "inlineChat.border": "#d5cbb8",

    "editorInlayHint.background": "#8040a020",
    "editorInlayHint.foreground": "#8040a0",

    "merge.currentHeaderBackground": "#3d7a2850",
    "merge.currentContentBackground": "#3d7a2825",
    "merge.incomingHeaderBackground": "#3060a050",
    "merge.incomingContentBackground": "#3060a025",

    "quickInput.background": "#faf8f2",
    "quickInputList.focusBackground": "#c8dba8",
    "quickInputTitle.background": "#f0ebe0",

    "notifications.background": "#faf8f2",
    "notifications.foreground": "#3a3225",
    "notifications.border": "#d5cbb8",
    "notificationCenterHeader.background": "#f0ebe0",

    "menu.background": "#faf8f2",
    "menu.foreground": "#3a3225",
    "menu.selectionBackground": "#c8dba8",
    "menu.selectionForeground": "#2a2520",
    "menu.separatorBackground": "#d5cbb8",
    "menubar.selectionBackground": "#c8dba8",

    "commandCenter.background": "#f0ebe0",
    "commandCenter.foreground": "#3a3225",
    "commandCenter.border": "#d5cbb8",
    "commandCenter.activeBackground": "#e8e0d5",
    "commandCenter.activeForeground": "#3a3225"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#8a917d", "fontStyle": "italic" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#c75820" }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#4a7a32" }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": { "foreground": "#b84040" }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": { "foreground": "#996b1f" }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#7a4a7d" }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#b8852a" }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": { "foreground": "#a83030" }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "meta.object-literal.key", "entity.name.tag.yaml"],
      "settings": { "foreground": "#6b5a45" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#7a6d5c" }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#7a6d5c" }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#6b5a45", "fontStyle": "italic" }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": { "foreground": "#b84040" }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#996b1f" }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": { "foreground": "#4a7a32" }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "Escape",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#b84040", "fontStyle": "underline" }
    },
    {
      "name": "Markdown Heading",
      "scope": ["heading.1.markdown", "markup.heading.setext.1", "entity.name.section.markdown"],
      "settings": { "foreground": "#c75820", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 2",
      "scope": ["heading.2.markdown", "markup.heading.setext.2"],
      "settings": { "foreground": "#b84040", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 3",
      "scope": ["heading.3.markdown"],
      "settings": { "foreground": "#996b1f", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 4",
      "scope": ["heading.4.markdown"],
      "settings": { "foreground": "#4a7a32", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 5",
      "scope": ["heading.5.markdown"],
      "settings": { "foreground": "#7a4a7d", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 6",
      "scope": ["heading.6.markdown"],
      "settings": { "foreground": "#4a6fa5", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#996b1f", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#7a4a7d", "fontStyle": "italic" }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "Markdown Link Text",
      "scope": ["string.other.link.title.markdown", "string.other.link.description.markdown"],
      "settings": { "foreground": "#4a7a32" }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": { "foreground": "#8a917d", "fontStyle": "italic" }
    },
    {
      "name": "Markdown List",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": { "foreground": "#c75820" }
    },
    {
      "name": "Cooklang Ingredient",
      "scope": ["entity.name.ingredient.cooklang", "punctuation.definition.ingredient.cooklang"],
      "settings": { "foreground": "#4a7a32", "fontStyle": "bold" }
    },
    {
      "name": "Cooklang Cookware",
      "scope": ["entity.name.cookware.cooklang", "punctuation.definition.cookware.cooklang"],
      "settings": { "foreground": "#b84040", "fontStyle": "bold" }
    },
    {
      "name": "Cooklang Timer",
      "scope": ["entity.name.timer.cooklang", "punctuation.definition.timer.cooklang"],
      "settings": { "foreground": "#7a4a7d" }
    },
    {
      "name": "Cooklang Amount",
      "scope": ["constant.numeric.cooklang"],
      "settings": { "foreground": "#b8852a" }
    },
    {
      "name": "Cooklang Unit",
      "scope": ["keyword.other.unit.cooklang"],
      "settings": { "foreground": "#996b1f" }
    },
    {
      "name": "Cooklang Metadata Key",
      "scope": ["entity.name.tag.metadata.cooklang"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "Cooklang Metadata Value",
      "scope": ["string.unquoted.metadata.cooklang"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "YAML Key",
      "scope": ["entity.name.tag.yaml"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "This/Self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": { "foreground": "#a83030", "fontStyle": "italic" }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.from", "keyword.control.export"],
      "settings": { "foreground": "#c75820" }
    },
    {
      "name": "Module",
      "scope": ["entity.name.module", "entity.name.namespace"],
      "settings": { "foreground": "#996b1f" }
    }
  ]
}
