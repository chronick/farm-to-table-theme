    "statusBarItem.hoverBackground": "#9a6858",
    "statusBarItem.prominentBackground": "#4a7a32",
    "statusBarItem.prominentForeground": "#faf5eb",
    "statusBarItem.remoteBackground": "#3a7a6a",
    "statusBarItem.remoteForeground": "#faf5eb",

    "tab.activeBackground": "#faf5eb",
    "tab.activeForeground": "#3d3629",
    "tab.inactiveBackground": "#e8dfd0",
    "tab.inactiveForeground": "#7a6d5c",
    "tab.border": "#d5cab8",
    "tab.activeBorder": "#faf5eb",
    "tab.activeBorderTop": "#4a7a32",
    "tab.unfocusedActiveBorderTop": "#7a9a62",
    "editorGroupHeader.tabsBackground": "#ddd2c2",
    "editorGroupHeader.tabsBorder": "#c9bda8",

    "input.background": "#faf5eb",
    "input.foreground": "#3d3629",
    "input.border": "#c9bda8",
    "input.placeholderForeground": "#9a9080",
    "inputOption.activeBorder": "#4a7a32",
    "inputOption.activeBackground": "#4a7a3230",
    "focusBorder": "#4a7a3280",

    "button.background": "#4a7a32",
    "button.foreground": "#faf5eb",
    "button.hoverBackground": "#5a8a42",
    "button.secondaryBackground": "#d0c4b0",
    "button.secondaryForeground": "#3d3629",
    "button.secondaryHoverBackground": "#c5b8a0",

    "dropdown.background": "#faf5eb",
    "dropdown.foreground": "#3d3629",
    "dropdown.border": "#c9bda8",
    "dropdown.listBackground": "#f5f0e5",

    "scrollbarSlider.background": "#b8a89060",
    "scrollbarSlider.hoverBackground": "#a8988080",
    "scrollbarSlider.activeBackground": "#988870",

    "badge.background": "#4a7a32",
    "badge.foreground": "#faf5eb",

    "terminal.background": "#f5f0e5",
    "terminal.foreground": "#3d3629",
    "terminal.border": "#d5cab8",
    "terminal.ansiBlack": "#3d3629",
    "terminal.ansiRed": "#b84040",
    "terminal.ansiGreen": "#4a7a32",
    "terminal.ansiYellow": "#996b1f",
    "terminal.ansiBlue": "#4a6fa5",
    "terminal.ansiMagenta": "#7a4a7d",
    "terminal.ansiCyan": "#3a7a6a",
    "terminal.ansiWhite": "#f4efe6",
    "terminal.ansiBrightBlack": "#7a6d5c",
    "terminal.ansiBrightRed": "#c75050",
    "terminal.ansiBrightGreen": "#5a8a42",
    "terminal.ansiBrightYellow": "#a87b2f",
    "terminal.ansiBrightBlue": "#5a7fb5",
    "terminal.ansiBrightMagenta": "#8a5a8d",
    "terminal.ansiBrightCyan": "#4a8a7a",
    "terminal.ansiBrightWhite": "#faf5eb",

    "gitDecoration.addedResourceForeground": "#4a7a32",
    "gitDecoration.modifiedResourceForeground": "#996b1f",
    "gitDecoration.deletedResourceForeground": "#b84040",
    "gitDecoration.untrackedResourceForeground": "#3a7a6a",
    "gitDecoration.ignoredResourceForeground": "#b8a890",
    "gitDecoration.conflictingResourceForeground": "#c75820",

    "editorError.foreground": "#b84040",
    "editorWarning.foreground": "#996b1f",
    "editorInfo.foreground": "#4a6fa5",
    "editorHint.foreground": "#3a7a6a",

    "peekView.border": "#4a7a32",
    "peekViewEditor.background": "#f5f0e5",
    "peekViewResult.background": "#ebe3d5",
    "peekViewTitle.background": "#e0d5c5",
    "peekViewTitleLabel.foreground": "#3d3629",

    "panel.background": "#e8e0d0",
    "panel.border": "#c9bda8",
    "panelTitle.activeBorder": "#4a7a32",
    "panelTitle.activeForeground": "#3d3629",
    "panelTitle.inactiveForeground": "#8a917d",
    "panelSectionHeader.background": "#ddd2c2",

    "breadcrumb.foreground": "#7a6d5c",
    "breadcrumb.focusForeground": "#3d3629",
    "breadcrumb.activeSelectionForeground": "#4a7a32",
    "breadcrumbPicker.background": "#f5f0e5",

    "editorWidget.background": "#f5f0e5",
    "editorWidget.border": "#c9bda8",
    "editorSuggestWidget.background": "#f5f0e5",
    "editorSuggestWidget.border": "#c9bda8",
    "editorSuggestWidget.selectedBackground": "#d8cdb8",
    "editorSuggestWidget.highlightForeground": "#4a7a32",
    "editorHoverWidget.background": "#f5f0e5",
    "editorHoverWidget.border": "#c9bda8",

    "minimap.background": "#f8f3e8",
    "minimap.selectionHighlight": "#4a7a3260",
    "minimapSlider.background": "#b8a89030",
    "minimapSlider.hoverBackground": "#b8a89050",
    "minimapSlider.activeBackground": "#b8a89070",

    "diffEditor.insertedTextBackground": "#4a7a3225",
    "diffEditor.removedTextBackground": "#b8404025",
    "diffEditorGutter.insertedLineBackground": "#4a7a3240",
    "diffEditorGutter.removedLineBackground": "#b8404040",

    "editorGhostText.foreground": "#8a917d80",

    "inlineChat.background": "#f5f0e5",
    "inlineChat.border": "#c9bda8",

    "editorInlayHint.background": "#7a4a7d20",
    "editorInlayHint.foreground": "#7a4a7d",

    "merge.currentHeaderBackground": "#4a7a3240",
    "merge.currentContentBackground": "#4a7a3220",
    "merge.incomingHeaderBackground": "#4a6fa540",
    "merge.incomingContentBackground": "#4a6fa520",

    "quickInput.background": "#f5f0e5",
    "quickInputList.focusBackground": "#d8cdb8",
    "quickInputTitle.background": "#e8e0d0",

    "notifications.background": "#f5f0e5",
    "notifications.foreground": "#3d3629",
    "notifications.border": "#c9bda8",
    "notificationCenterHeader.background": "#e8e0d0",

    "menu.background": "#f5f0e5",
    "menu.foreground": "#4a4035",
    "menu.selectionBackground": "#d8cdb8",
    "menu.selectionForeground": "#3d3629",
    "menu.separatorBackground": "#c9bda8",
    "menubar.selectionBackground": "#d8cdb8",

    "commandCenter.background": "#e8e0d0",
    "commandCenter.foreground": "#4a4035",
    "commandCenter.border": "#c9bda8",
    "commandCenter.activeBackground": "#ddd2c2",
    "commandCenter.activeForeground": "#3d3629"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#8a917d", "fontStyle": "italic" }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#c75820" }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#4a7a32" }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": { "foreground": "#b84040" }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": { "foreground": "#996b1f" }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#7a4a7d" }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#b8852a" }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": { "foreground": "#a83030" }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "meta.object-literal.key", "entity.name.tag.yaml"],
      "settings": { "foreground": "#6b5a45" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#7a6d5c" }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#7a6d5c" }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#6b5a45", "fontStyle": "italic" }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": { "foreground": "#b84040" }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#996b1f" }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": { "foreground": "#4a7a32" }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "Escape",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#b84040", "fontStyle": "underline" }
    },
    {
      "name": "Markdown Heading",
      "scope": ["heading.1.markdown", "markup.heading.setext.1", "entity.name.section.markdown"],
      "settings": { "foreground": "#c75820", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 2",
      "scope": ["heading.2.markdown", "markup.heading.setext.2"],
      "settings": { "foreground": "#b84040", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 3",
      "scope": ["heading.3.markdown"],
      "settings": { "foreground": "#996b1f", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 4",
      "scope": ["heading.4.markdown"],
      "settings": { "foreground": "#4a7a32", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 5",
      "scope": ["heading.5.markdown"],
      "settings": { "foreground": "#7a4a7d", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Heading 6",
      "scope": ["heading.6.markdown"],
      "settings": { "foreground": "#4a6fa5", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#996b1f", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#7a4a7d", "fontStyle": "italic" }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "Markdown Link Text",
      "scope": ["string.other.link.title.markdown", "string.other.link.description.markdown"],
      "settings": { "foreground": "#4a7a32" }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": { "foreground": "#8a917d", "fontStyle": "italic" }
    },
    {
      "name": "Markdown List",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": { "foreground": "#c75820" }
    },
    {
      "name": "Cooklang Ingredient",
      "scope": ["entity.name.ingredient.cooklang", "punctuation.definition.ingredient.cooklang"],
      "settings": { "foreground": "#4a7a32", "fontStyle": "bold" }
    },
    {
      "name": "Cooklang Cookware",
      "scope": ["entity.name.cookware.cooklang", "punctuation.definition.cookware.cooklang"],
      "settings": { "foreground": "#b84040", "fontStyle": "bold" }
    },
    {
      "name": "Cooklang Timer",
      "scope": ["entity.name.timer.cooklang", "punctuation.definition.timer.cooklang"],
      "settings": { "foreground": "#7a4a7d" }
    },
    {
      "name": "Cooklang Amount",
      "scope": ["constant.numeric.cooklang"],
      "settings": { "foreground": "#b8852a" }
    },
    {
      "name": "Cooklang Unit",
      "scope": ["keyword.other.unit.cooklang"],
      "settings": { "foreground": "#996b1f" }
    },
    {
      "name": "Cooklang Metadata Key",
      "scope": ["entity.name.tag.metadata.cooklang"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "Cooklang Metadata Value",
      "scope": ["string.unquoted.metadata.cooklang"],
      "settings": { "foreground": "#3a7a6a" }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "YAML Key",
      "scope": ["entity.name.tag.yaml"],
      "settings": { "foreground": "#4a6fa5" }
    },
    {
      "name": "This/Self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": { "foreground": "#a83030", "fontStyle": "italic" }
    },
    {
      "name": "Import",
      "scope": ["keyword.control.import", "keyword.control.from", "keyword.control.export"],
      "settings": { "foreground": "#c75820" }
    },
    {
      "name": "Module",
      "scope": ["entity.name.module", "entity.name.namespace"],
      "settings": { "foreground": "#996b1f" }
    }
  ]
}
